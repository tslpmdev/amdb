<h1>New Actor</h1>


<%= form_tag(actors_url, method: 'post') do %>


  <div>
    <%= label_tag :name %><br />
    <%= text_field_tag :name %>
  </div>

    <div>
    <%= label_tag :dob %><br />
    <%= text_field_tag :dob %>
  </div>

  
  <div>
    <%= submit_tag "Create Actor" %>
  </div>
<% end %>
